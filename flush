roslaunch trajectory_follower mpc.launch
roslaunch trajectory_follower navigate_status.launch

roslaunch change_inflation change_inflation.launch
roslaunch freespace_detection free_space.launch

roslaunch robot_diagnostic robot_diagnostic.launch

roslaunch decision_maker decision_maker.launch 
roslaunch has_local_planner planning_costmap.launch
roslaunch loam_localization loam_locate.launch
rosrun robot_upstart install chiturobot_bringup/launch/A_robot_upstart.launch

rosrun robot_upstart install chitu_bringup/autogo_drivers.launch
rosrun robot_upstart install asp_bringup/launch/cloud_platform.launch
 sudo systemctl daemon-reload && sudo systemctl start chitu
 
sudo systemctl stop chitu
sudo systemctl stop asp
 
roslaunch chitu_bringup autogo_drivers.launch
roslaunch asp_bringup cloud_platform.launch



